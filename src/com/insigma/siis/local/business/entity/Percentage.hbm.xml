<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
  <class name="com.insigma.siis.local.business.entity.Percentage"
    lazy="false"
    table="PERCENTAGE">
    <id column="IDS" length="200" name="ids" type="java.lang.String" >
      <generator class="assigned" />
    </id>
    <property column="sessionID" length="200" name="sessionID" type="java.lang.String" />
	<property column="A0000" length="200" name="a0000" type="java.lang.String" />
	<property column="PERCENTAGE_NUMBER" length="200" name="percentage_number" type="java.lang.String" />
  </class>
</hibernate-mapping>
