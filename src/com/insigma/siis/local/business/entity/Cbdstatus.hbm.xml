<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
  <class name="com.insigma.siis.local.business.entity.Cbdstatus"
    lazy="false"
    table="CBD_STATUS">
    <id column="STATUS_ID" name="statusid" type="java.lang.String">
      <generator class="assigned" />
    </id>
    <property column="CBD_ID" length="60" name="cbdid" type="java.lang.String" />
    <property column="CBD_STATUS_STEP" length="2" name="cbdstatusstep" type="java.lang.String" />
    <property column="CBD_STATUS_TIME" length="32" name="cbdstatustime" type="java.lang.String" />
  </class>
</hibernate-mapping>
