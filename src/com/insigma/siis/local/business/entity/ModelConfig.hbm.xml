<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
  <class name="com.insigma.siis.local.business.entity.ModelConfig"
    lazy="false"
    table="ModelConfig">
  	 <id name="id" type="java.lang.String">
            <column name="ID" length="60" />
            <generator class="assigned" />
      </id>
     <property name="expertise" type="java.lang.String">
            <column name="EXPERTISE" length="50">
                <comment>专长</comment>
            </column>
     </property>
    
     <property name="region" type="java.lang.String">
            <column name="REGION" length="10">
                <comment>地域</comment>
            </column>
     </property>
     <property name="ability" type="java.lang.String">
            <column name="ABILITY" length="10">
                <comment>能力</comment>
            </column>
     </property>
     <property name="experience" type="java.lang.String">
            <column name="EXPERIENCE" length="10">
                <comment>经验</comment>
            </column>
     </property>
     <property name="undergo" type="java.lang.String">
            <column name="UNDERGO" length="10">
                <comment>经历</comment>
            </column>
     </property>
     <property name="source" type="java.lang.String">
            <column name="SOURCE" length="10">
                <comment>来源</comment>
            </column>
     </property>
     <property name="tenuresystem" type="java.lang.String">
            <column name="TENURESYSTEM" length="10">
                <comment>任期制</comment>
            </column>
     </property>
   
  </class>
</hibernate-mapping>
