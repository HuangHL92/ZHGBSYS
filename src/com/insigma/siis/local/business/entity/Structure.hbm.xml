                <comment>备注</comment>
            </column>
     </property>
     <property name="zttype" type="java.lang.String">
            <column name="ZTTYPE" length="20">
                <comment>整体状况类型</comment>
            </column>
     </property>
     <property name="znzstype" type="java.lang.String">
            <column name="ZNZSTYPE" length="20">
                <comment>职能职数状况类型</comment>
            </column>
     </property>
     <property name="nljgtype" type="java.lang.String">
            <column name="NLJGTYPE" length="20">
                <comment>年龄结构状况类型</comment>
            </column>
     </property>
     <property name="xljgtype" type="java.lang.String">
