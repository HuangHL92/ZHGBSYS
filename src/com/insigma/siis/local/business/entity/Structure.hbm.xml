     <property name="neljgtype" type="java.lang.String">
            <column name="NELJGTYPE" length="20">
                <comment>能力结构状况类型</comment>
            </column>
     </property>
     <property name="jyjgtype" type="java.lang.String">
            <column name="JYJGTYPE" length="20">
                <comment>经验结构状况类型</comment>
            </column>
     </property>
     <property name="lyjgtype" type="java.lang.String">
            <column name="LYJGTYPE" length="20">
                <comment>来源结构状况类型</comment>
            </column>
     </property>
     <property name="rqztype" type="java.lang.String">
            <column name="RQZTYPE" length="20">
