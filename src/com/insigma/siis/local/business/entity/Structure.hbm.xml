            </column>
     </property>
     <property name="zcjgtype" type="java.lang.String">
            <column name="ZCJGTYPE" length="20">
                <comment>专长结构状况类型</comment>
            </column>
     </property>
     <property name="dyjgtype" type="java.lang.String">
            <column name="DYJGTYPE" length="20">
                <comment>地域结构状况类型</comment>
            </column>
     </property>
     <property name="xbjgtype" type="java.lang.String">
