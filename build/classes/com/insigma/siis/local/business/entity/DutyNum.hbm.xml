<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
  <class name="com.insigma.siis.local.business.entity.DutyNum"
    lazy="false"
    table="duty_num">
  	<id column="ID" name="id" type="java.lang.String">
      <generator class="uuid" />
    </id>
     <property name="orderNumber" type="java.lang.Integer">
            <column name="ORDER_NUMBER" length="20">
                <comment>序号</comment>
            </column>
     </property>
     <property name="unit" type="java.lang.String">
            <column name="UNIT" length="20">
                <comment>所属单位代码</comment>
            </column>
     </property>
     <property name="project" type="java.lang.String">
            <column name="PROJECT" length="20">
                <comment>项目</comment>
            </column>
     </property>
     <property name="dutyCategory" type="java.lang.String">
            <column name="DUTY_CATEGORY" length="20">
                <comment>职务类别</comment>
            </column>
     </property>
     <property name="dutyRank" type="java.lang.String">
            <column name="DUTY_RANK" length="20">
                <comment>职务职级</comment>
            </column>
     </property>
     <property name="quantity" type="java.lang.String">
            <column name="QUANTITY" length="20">
                <comment>数量</comment>
            </column>
     </property>
     <property name="remark" type="java.lang.String">
            <column name="REMARK" length="20">
                <comment>备注</comment>
            </column>
     </property>
     <property name="oneTicketVeto" type="java.lang.String">
            <column name="ONE_TICKET_VETO" length="20">
                <comment>一票否决</comment>
            </column>
     </property>
     <property name="querysql" type="java.lang.String">
            <column name="QUERYSQL" length="20">
                <comment>查询数量的sql</comment>
            </column>
     </property>
     <property name="creattime" type="java.lang.String">
            <column name="CREATTIME" length="20">
                <comment>设计时间</comment>
            </column>
     </property>

  </class>
</hibernate-mapping>
