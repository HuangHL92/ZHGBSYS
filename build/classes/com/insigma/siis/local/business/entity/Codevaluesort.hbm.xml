<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.insigma.siis.local.business.entity.Codevaluesort"
    lazy="false"
    table="CODEVALUE_SORT">
    <id column="ID" name="id" type="java.lang.Long">
      <generator class="assigned" />
    </id>
    <property column="CODE_TYPE" length="200" name="codetype" type="java.lang.String" />
    <property column="CODE_VALUE" length="100" name="codevalue" type="java.lang.String" />
    <property column="SORTVALUE" length="200" name="sortvalue" type="java.lang.String" />
  </class>
</hibernate-mapping>
