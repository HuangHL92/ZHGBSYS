<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
  <class name="com.insigma.siis.local.business.entity.Sysmodelrole"
    lazy="false"
    table="SYSMODELROLE">
    <id column="MODELROLEID" name="modelroleid" type="java.lang.String">
      <generator class="uuid" />
    </id>
    <property column="SUB_LIBRARIES_MODEL_ID" length="32" name="sublibrariesmodelid" type="java.lang.String" />
    <property column="USERID" length="32" name="userid" type="java.lang.String" />
  </class>
</hibernate-mapping>
