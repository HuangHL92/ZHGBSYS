<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
  <class name="com.insigma.siis.local.business.entity.Inf"
    lazy="false"
    table="COMPETENCE_INF">
    <id column="INFOID" name="infoid" type="java.lang.String">
      <generator class="assigned" />
    </id>
    <property column="INFONAME" length="50" name="infoname" type="java.lang.String" />
    <property column="INFOTYPE" length="2" name="infotype" type="java.lang.String" />
    <property column="CODE_TYPE" length="20" name="codetype" type="java.lang.String" />
  </class>
</hibernate-mapping>
