<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.insigma.siis.local.business.entity.Userauthentication"
    lazy="false"
    table="USER_AUTHENTICATION">
    <id column="TOKEN" name="token" type="java.lang.String">
      <generator class="assigned" />
    </id>
    <property column="USERNAME" length="20" name="username" type="java.lang.String" />
  </class>
</hibernate-mapping>
