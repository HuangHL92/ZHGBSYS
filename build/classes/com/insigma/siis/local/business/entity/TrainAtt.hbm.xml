	</id>
	<property column="TRAINID" length="120" name="trainid" type="java.lang.String" />
	<property column="G11031" length="200" name="g11031" type="java.lang.String" />
	<property column="G11052" length="200" name="g11052" type="java.lang.String" />
	<property column="G11053" length="30" name="g11053" type="java.lang.String" />
	<property column="USERID" length="120" name="userid" type="java.lang.String" />
